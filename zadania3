import java.util.Random;
import java.util.Scanner;

public class lab3 {
    public static void main(String args[]){
        Scanner s = new Scanner(System.in);
        System.out.print("Podaj n:");
        int x = s.nextInt();
        Random r = new Random();
        System.out.print("Losowe liczby z przedzia≈Çu -999 do 999: \n");
        for(int i = 0; i < x ; i++){
            System.out.println(r.nextInt(999 + 999) -999);





        }





    }
}
